<local>
	<outputs>
		<output id="0">
			<!--Check network connection-->
			<title>Vérifiez votre connexion réseau.</title>
			<!--Unable to check for software updates at this time!-->
			<message>Impossible de vérifier les mises à jours logicielles pour le moment !</message>
		</output>
		<output id="1">
			<!--No internet connction-->
			<title>Aucune connexion réseau.</title>
			<!--Updates could not be verified-->
			<message>Les mises à jour logicielles n'ont pas pu être vérifiées !</message>
		</output>
		<output id="2">
			<!--{App} will be updated-->
			<title>{0} va être mis à jour !</title>
			<!--{v1.0} to {v2.0}-->
			<message>{0} vers {1}</message>
		</output>
		<output id="3">
			<!--{App} updated-->
			<title>{0} a été mis à jour.</title>
			<!--Installed version: {v1.0}-->
			<message>Version installée : {0}</message>
		</output>
		<output id="4">
			<!--{App} could not be updated-->
			<title>{0} n'a pas pu être mis à jour !</title>
			<!--Please contact support-->
			<message>Contacter le support.</message>
		</output>
		<output id="5">
			<!--Logs are not available yet-->
			<message>Les logs ne sont pas encore disponibles</message>
		</output>
		<output id="6">
			<!--Starting a manual check for updated apps-->
			<message>Recherche de mises à jour logicielles en cours...</message>
		</output>
		<output id="7">
			<!--Couldn't start a manual check for updated apps-->
			<message>La recherche de mises à jour logicielles n'a pas pu démarrer !</message>
		</output>
		<output id="8">
			<!--Check for updated apps already running-->
			<message>La recherche de mises à jour est déjà en cours...</message>
		</output>
		<output id="9">
			<!--Manual check for updated apps completed-->
			<message>Recherche de mises à jour terminée.</message>
		</output>
		<output id="10">
			<!--See changelog-->
			<message>Consulter le journal des modifications</message>
		</output>
		<output id="11">
			<!--Open log file-->
			<message>Ouvrir le fichier log</message>
		</output>
	</outputs>
</local>
